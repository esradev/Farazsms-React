{
  "name": "farazsms-plugin",
  "version": "2.0.0",
  "description": "By using the Faraz SMS plugin, you can professionally equip your site with a powerful SMS tool for information and marketing. Saving customers numbers in the phone book, sending welcome SMS, sending reply SMS to comments, etc. are part of the features of this powerful SMS plugin.",
  "main": "index.js",
  "scripts": {
    "dev:build": "concurrently --kill-others \"npm run build\" \"npm run build:blocks\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "build:blocks": "wp-scripts build src/blocks/newsletter/index.js src/blocks/newsletter/frontend.js --output-path=build-blocks",
    "start:blocks": "wp-scripts build src/blocks/newsletter/index.js src/blocks/newsletter/frontend.js --output-path=build-blocks",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esradev/Farazsms-React.git"
  },
  "author": "wpstorm",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/esradev/FarazSmsPlugin/issues"
  },
  "homepage": "https://github.com/esradev/FarazSmsPlugin#readme",
  "devDependencies": {
    "@wordpress/scripts": "^24.6.0",
    "axios": "^1.2.0",
    "browser-sync-webpack-plugin": "^2.3.0",
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@wordpress/i18n": "^4.23.0",
    "bootstrap": "^5.2.3",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.7.0",
    "react-color": "^2.19.3",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.5",
    "react-select": "^5.7.0",
    "react-transition-group": "^4.4.5",
    "use-immer": "^0.8.1",
    "webpack": "^5.75.0"
  }
}
